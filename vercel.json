{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/carrito", "destination": "/" }
  ],
  "redirects": [
    { "source": "/panel", "destination": "/panel.html", "statusCode": 308 },
    { "source": "/admin", "destination": "/admin.html", "statusCode": 308 }
  ]
}
